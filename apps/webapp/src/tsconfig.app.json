{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../out-tsc/app",
    "target": "es5",
    "module": "es2015",
    "baseUrl": ".",
    "types": [
      "webpack-env"
    ],
    "paths": {
      "@angular/*": [ "../node_modules/@angular/*" ],
      "@ngx-plus/*": [ "../node_modules/@ngx-plus/*" ],
      "@ngrx/*": [ "../node_modules/@ngrx/*" ],
      "rxjs/*": [ "../node_modules/rxjs/*" ]
    },
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
